{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.12",
        "aws/aws-sdk-php-laravel": "^3.4.0",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "doctrine/dbal": "^2.10.0",
        "euautomation/graphql-client": "^0.2.0",
        "guzzlehttp/guzzle": "^6.4.1",
        "laravel/framework": "^6.6.0",
        "laravel/tinker": "^2.0.0",
        "laravelcollective/html": "^6.0.3",
        "league/flysystem-aws-s3-v3": "^1.0.23",
        "mews/captcha": "^3.0.1",
        "nixilla/php-jwt": "^0.1.1",
        "predis/predis": "^1.1.1",
        "soundasleep/html2text": "^1.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.0",
        "mockery/mockery": "^1.3.0",
        "phpunit/phpunit": "^8.4.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    }
}
